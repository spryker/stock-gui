  "type": "library",
  "description": "StockGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/stock": "^8.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-text": "^1.2.0"
